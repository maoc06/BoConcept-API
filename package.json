{
    "author": "Miguel Orrego",
    "license": "ISC",
    "main": "index.js",
    "name": "boconcept-api",
    "version": "1.0.0",
    "description": "RESTful API for BoConcept Furniture",
    "engines": {
        "node": "12.18.2"
    },
    "scripts": {
        "start": "node .",
        "build": "babel --delete-dir-on-start --out-dir dist --copy-files --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored src",
        "test": "mocha --timeout 15000",
        "lint": "eslint ./",
        "lint:fix": "eslint ./ --fix",
        "swagger-autogen": "node src/swagger.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test && npm run lint:fix"
        }
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "morgan": "^1.10.0",
        "pg": "^8.4.2",
        "swagger-autogen": "^1.2.5",
        "swagger-ui-express": "^4.1.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.7",
        "@babel/core": "^7.12.7",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "@babel/register": "^7.12.1",
        "babel-eslint": "^10.1.0",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-node": "^4.1.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.0.0",
        "husky": "^4.3.0",
        "mocha": "^8.2.1",
        "nodemon": "^2.0.6",
        "prettier": "^2.2.0",
        "should": "^13.2.3"
    }
}
